// vim: set ft=json:
{
  "extends": ["airbnb", "plugin:prettier/recommended", "prettier/react"],
  "plugins": ["react", "jsx-a11y", "import", "prettier", "react-hooks"],
  "settings": {
    "react": {
      "version": "16.9.0"
    }
  },
  "rules": {
      "react/jsx-props-no-spreading": [0,{
          "html" : "ignore",
          "custom": "ignore",
          "exceptions": []
      }] ,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".tsx"]
      }
    ]
  }
}
